{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/Order",
    "definitions": {
        "Order": {
            "properties": {
                "order_id": {
                    "type": "string"
                },
                "user_id": {
                    "type": "string"
                },
                "item": {
                    "$ref": "#/definitions/example.Order.Item",
                    "additionalProperties": true
                },
                "status": {
                    "enum": [
                        "PENDING",
                        0,
                        "SHIPPED",
                        1,
                        "DELIVERED",
                        2
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Status"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Order"
        },
        "example.Order.Item": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "price": {
                    "type": "number"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Item"
        }
    }
}